{
  "type": "commonjs",
  "version": "1.0.0",
  "name": "my-webpack-project",
  "description": "My webpack project",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.ts",
    "build:server": "webpack --config webpack.server.ts",
    "start": "npm run build:server && node dist/server/index.js",
    "dev": "babel-node --extensions '.tsx,.ts,.js' src/server/index.ts",
    "nm": "nodemon",
    "watch": "webpack --watch",
    "serve": "webpack serve --config webpack.client.ts --mode development"
  },
  "nodemonConfig": {
    "exec": "npm run build && node dist/server/index.js --dev",
    "ext": "ts,tsx",
    "ignore": [
      "**/dist/**",
      "**/node_modules/**",
      "src/client/**"
    ],
    "watch": "src/server"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "@fastify/vite": "^4.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "devalue": "^4.3.2",
    "fastify": "^4.18.0",
    "html-entities": "^2.4.0",
    "jotai": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "babel-loader": "^9.1.2",
    "esbuild-loader": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
