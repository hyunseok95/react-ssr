{
  "type": "commonjs",
  "version": "1.0.0",
  "name": "my-webpack-project",
  "description": "My webpack project",
  "scripts": {
    "prebuild": "rimraf dist",
    "build2": "webpack --mode=production --node-env=production",
    "build3": "npm run build:client1 && npm run build:client2",
    "build": "npm run build:client && npm run build:server",
    "build:client1": "vite build --outDir ../../dist/client --ssrManifest ",
    "build:client2": "vite build --outDir ../../dist/client --ssr ./index",
    "build:client": "webpack --config webpack.client.ts",
    "build:server": "webpack --config webpack.server.ts",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "start": "npm run build:server && node dist/server/index.js",
    "dev": "babel-node --extensions '.ts,.js' src/server/server.ts",
    "nm": "nodemon",
    "w1": "webpack",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "nodemonConfig": {
    "exec": "npm run build && node dist/server/index.js --dev",
    "ext": "ts,tsx",
    "ignore": [
      "**/dist/**",
      "**/node_modules/**",
      "src/client/**"
    ],
    "watch": "src/server"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "@fastify/vite": "^4.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "devalue": "^4.3.2",
    "fastify": "^4.18.0",
    "jotai": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "babel-loader": "^9.1.2",
    "esbuild-loader": "^3.0.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
